{
  "tabWidth": 2,
  "useTabs": false,
  "singleQuote": false,
  "semi": true,
  "bracketSpacing": true,
  "trailingComma": "es5",
  "bracketSameLine": true,
  "printWidth": 160,
  "endOfLine": "crlf",
  "importOrder": ["^(react)(.*)$", "^(@mui)(.*)$", "<THIRD_PARTY_MODULES>", "^[./]"],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true,
  "linebreak-style": "crlf"
}
